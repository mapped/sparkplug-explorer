{
  "name": "sparkplug-explorer",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "vite build && tsc -p .",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 node dist/index.js",
    "dev": "NODE_OPTIONS=--max-old-space-size=8192 npx tsx watch --clear-screen=false src/index.ts",
    "dev:tsnode": "NODE_OPTIONS=--max-old-space-size=8192 ts-node src/index.ts",
    "dev:webonly": "NODE_OPTIONS=--max-old-space-size=8192 DISABLE_SPARKPLUG=1 tsx watch --clear-screen=false src/index.ts",
    "report": "NODE_OPTIONS=--max-old-space-size=8192 ts-node src/report.ts",
    "ui:build": "vite build"
  },
  "dependencies": {
    "@duckdb/node-api": "^1.3.2-alpha.25",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^7.0.4",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/x-data-grid": "^6.19.6",
    "fastify": "^5.4.0",
    "long": "^5.2.3",
    "mqtt": "^5.8.1",
    "pako": "^2.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "sparkplug-payload": "^1.0.3",
    "react-window": "^1.8.10"
  },
  "devDependencies": {
    "@fastify/middie": "^9.0.3",
    "@types/node": "^20.0.0",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-window": "^1.8.5",
    "@vitejs/plugin-react": "^4.3.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "^5.4.0",
    "vite": "^6.4.1"
  }
}
